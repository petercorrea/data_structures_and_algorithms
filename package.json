{
  "scripts": {
    "test": "jest --config ./jest.config.json",
    "lint": "eslint --fix --ext .js,.jsx .",
    "pre-commit": "pretty-quick --staged",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,css,ts}\"",
    "prepare": "husky install",
    "spell-check": "cspell --words-only --unique \"**\" | sort --ignore-case >> cspell.txt",
    "clean": "npm run lint && npm run pretty && npm run test && npm run spell-check"
